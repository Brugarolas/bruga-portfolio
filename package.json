{
  "name": "bruga-portfolio",
  "description": "A personal portfolio made with Pug and Sass",
  "version": "1.1.0",
  "author": "andres-brugarolas",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --hot",
    "start:local": "cross-env NODE_ENV=development LOCAL=true webpack serve --hot",
    "start:static": "npm run build && http-server ./dist",
    "build": "cross-env NODE_ENV=production webpack --progress --performance",
    "analyzer": "cross-env NODE_ENV=production ANALYZER=true webpack --progress --performance",
    "add-icon": "node scripts/download-fa-icon",
    "update-included-icons": "node scripts/download-included-icons"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "dayjs": "^1.9.6",
    "raf": "3.4.1",
    "nanoid": "^3.1.16",
    "typewriter-effect": "^2.13.1",
    "@fortawesome/fontawesome-free": "^5.15.1"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not ie <= 10",
    "not dead"
  ],
  "devDependencies": {
    "cross-env": "^7.0.2",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "babel-loader": "^8.2.1",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "babel-preset-minify": "^0.5.1",
    "file-loader": "^6.2.0",
    "css-loader": "^5.0.1",
    "sass-loader": "^10.1.0",
    "pug-loader": "^2.4.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.1",
    "webfonts-loader": "^7.2.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "sass": "^1.29.0",
    "fibers": "^5.0.0",
    "pug": "^3.0.0",
    "svgo": "^2.3.0",
    "@supercharge/promise-pool": "^1.7.0",
    "puppeteer-core": "^9.0.0",
    "http-server": "^0.12.3",
    "chalk": "^4.1.1"
  }
}
